package com.kkl.kklplus.b2b.joyoung.http.request;

import lombok.Data;

import java.util.Date;

@Data
public class OrderVisitedRequestParam extends RequestParam{

    /**
     * 必填，工单号
     */
    private String orderNo = "";

    /**
     * 必填，上门人
     */
    private String visitMan = "";

    /**
     * 必填，上门时间(格式：yyyy-MM-dd hh:mm:ss)
     */
    private Long visitDate;

    /**
     * 必填，上门备注信息
     */
    private String remarks = "";
}
